# Generated by sweethone3d-windows-builder on {{ generation_date }}
#

id={{ library.id }}
name={{ library.name }}
description={{ library.description }}
version={{ library.version }}
license={{ library.license }}
provider={{ library.provider }}

{% for window in library.windows %}
id#{{ loop.index }}={{ library.id }}#{{ window.id }}
name#{{ loop.index }}={{ window.name }}
tags#{{ loop.index }}=Window
category#{{ loop.index }}={{ window.category }}
icon#{{ loop.index }}=/{{ window.icon_file_archive_path }}
model#{{ loop.index }}=/{{ window.obj_file_archive_path }}
multiPartModel#{{ loop.index }}=true
width#{{ loop.index }}={{ window.width }}
depth#{{ loop.index }}={{ window.depth }}
height#{{ loop.index }}={{ window.height }}
dropOnTopElevation#{{ loop.index }}=-1
elevation#{{ loop.index }}={{ window.elevation }}
movable#{{ loop.index }}=false
doorOrWindow#{{ loop.index }}=true
doorOrWindowCutOutShape#{{ loop.index }}=M0,0 v1 h1 v-1 z
doorOrWindowWallThickness#{{ loop.index }}={{ window.wall_thickness }}
doorOrWindowSashXAxis#{{ loop.index }}={{ window.sashs_attr('x_axis') }}
doorOrWindowSashYAxis#{{ loop.index }}={{ window.sashs_attr('y_axis') }}
doorOrWindowSashWidth#{{ loop.index }}={{ window.sashs_attr('width') }}
doorOrWindowSashStartAngle#{{ loop.index }}={{ window.sashs_attr('start_angle') }}
doorOrWindowSashEndAngle#{{ loop.index }}={{ window.sashs_attr('end_angle') }}
modelSize#{{ loop.index }}={{ window.obj_file_size }}
horizontallyRotatable#{{ loop.index }}=false
creator#{{ loop.index }}={{ library.creator }}
{% endfor %}